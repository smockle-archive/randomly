{
  "name": "randomly",
  "version": "1.0.1",
  "description": "Randomly generate and sort data.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node index.js",
    "build": "babel src --out-dir dist",
    "docs": "jsdoc2md \"dist/**/*.js\" --template jsdoc2md/README.hbs > README.md",
    "test": "tape \"tests/**/*.js\" && snyk test",
    "coverage": "istanbul cover tape \"tests/**/*.js\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/randomly.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/randomly/issues"
  },
  "homepage": "https://github.com/smockle/randomly#readme",
  "devDependencies": {
    "codeclimate-test-reporter": "^0.3.1",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.2",
    "jsdoc-to-markdown": "^1.3.3",
    "tape": "^4.4.0"
  },
  "snyk": true,
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-register": "^6.5.2",
    "snyk": "^1.14.3"
  }
}
