{
  "name": "randomly",
  "version": "3.0.1",
  "description": "Randomly generate and sort data.",
  "main": "lib/randomly.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "docs": "jsdoc2md lib/randomly.mjs --configure jsdoc.json --template jsdoc2md/README.hbs --example-lang JavaScript > README.md",
    "test": "node -r @std/esm test/randomly.mjs | colortape",
    "coverage": "nyc --require @std/esm --extension .mjs --reporter=lcov node test/randomly.mjs | colortape",
    "postcoverage": "nyc report",
    "prepublish": "npm test",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/randomly.git"
  },
  "author": "Clay Miller <clay@smockle.com> (https://smockle.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smockle/randomly/issues"
  },
  "homepage": "https://github.com/smockle/randomly#readme",
  "devDependencies": {
    "codecov": "^2.2.0",
    "colortape": "^0.1.2",
    "eslint": "^4.1.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "flow-bin": "^0.53.1",
    "greenkeeper-postpublish": "^1.0.0",
    "husky": "^0.14.0",
    "jsdoc-to-markdown": "^3.0.0",
    "lint-staged": "^4.0.0",
    "nyc": "^11.0.2",
    "prettier-eslint-cli": "^4.0.4"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/prettier-eslint --write",
      "git add"
    ]
  },
  "@std/esm": {
    "cjs": true
  },
  "dependencies": {
    "@std/esm": "^0.5.1"
  }
}
